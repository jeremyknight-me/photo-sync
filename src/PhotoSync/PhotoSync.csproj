<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net5.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
        <UseWindowsForms>true</UseWindowsForms>
        <RuntimeIdentifier>win-x64</RuntimeIdentifier>
        <SelfContained>false</SelfContained>
        <PublishSingleFile>true</PublishSingleFile>
        <PublishTrimmed>false</PublishTrimmed>
        <PublishReadyToRun>false</PublishReadyToRun>
        <Authors>Jeremy Knight</Authors>
        <Company></Company>
        <Description>PhotoSync is a tool to manage photo syncs.</Description>
        <RepositoryUrl>https://github.com/jeremyknight-me/photo-sync</RepositoryUrl>
        <PackageProjectUrl>https://github.com/jeremyknight-me/photo-sync</PackageProjectUrl>
        <Copyright>Jeremy Knight</Copyright>
        <Version>1.0.0</Version>
    </PropertyGroup>
    <ItemGroup>
      <Folder Include="UserControls\" />
    </ItemGroup>
</Project>
