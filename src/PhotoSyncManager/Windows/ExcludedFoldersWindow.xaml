<Window x:Class="PhotoSyncManager.Windows.ExcludedFoldersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotoSyncManager.Windows"
        mc:Ignorable="d"
        Title="Excluded Folders" Height="350" Width="300">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition Height="9*" />
            <RowDefinition Height="1*" />
        </Grid.RowDefinitions>

        <ListBox Grid.Row="0">
        </ListBox>

        <StackPanel Grid.Row="1" HorizontalAlignment="Right" Orientation="Horizontal" Margin="0 5 0 0">
            <Button Content="Close" Margin="0 0 5 0"></Button>
            <Button Content="Save"></Button>
        </StackPanel>
    </Grid>
</Window>
